{
	"name": "dartsmate-admin",
	"version": "0.1.0",
	"private": true,
	"config": {
		"imageName": "dartsmate-admin",
		"devEcrUrl": "007527336698.dkr.ecr.ap-northeast-2.amazonaws.com/dartsmate/dev/admin",
		"ecrUrl": "007527336698.dkr.ecr.ap-northeast-2.amazonaws.com/dartsmate/prd/admin"
	},
	"scripts": {
		"local": "NODE_ENV=local vite dev",
		"dev": "vite dev",
		"clean": "rm -rf node_modules && rm -f package-lock.json && rm -rf build && npm cache clean --force && npm install && npx patch-package && npm run prisma",
		"prisma": "npx prisma generate",
		"build:dev": "npm run clean && vite build --mode development",
		"build": "npm run clean && vite build",
		"start:local": "NODE_ENV=local ORIGIN=http://localhost:3000 AWS_PROFILE=dm-dev-seoul node custom-server.js",
		"start:dev": "NODE_ENV=development ORIGIN=https://admin.dm-dev.battledartsgames.com node custom-server.js",
		"start": "NODE_ENV=production ORIGIN=https://admin.dm.battledartsgames.com node custom-server.js",
		"docker-build": "docker buildx build --platform linux/amd64 -t $npm_package_config_imageName:$npm_package_version .",
		"docker-tag:dev": "docker tag $npm_package_config_imageName:$npm_package_version $npm_package_config_devEcrUrl:$npm_package_version",
		"docker-tag": "docker tag $npm_package_config_imageName:$npm_package_version $npm_package_config_ecrUrl:$npm_package_version",
		"docker-push:dev": "docker push $npm_package_config_devEcrUrl:$npm_package_version",
		"docker-push": "docker push $npm_package_config_ecrUrl:$npm_package_version",
		"docker:dev": "npm run docker-build && npm run docker-tag:dev && npm run docker-push:dev",
		"docker": "npm run docker-build && npm run docker-tag && npm run docker-push",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/nodemailer": "^6.4.14",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"autoprefixer": "^10.4.14",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-svelte": "^2.30.0",
		"postcss": "^8.4.24",
		"postcss-load-config": "^4.0.1",
		"prettier": "^3.0.0",
		"prettier-plugin-tailwindcss": "^0.4.1",
		"prisma": "^5.7.0",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"sveltekit-flash-message": "^2.3.1",
		"sveltekit-superforms": "^1.12.0",
		"tailwindcss": "^3.3.2",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.0",
		"zod": "^3.22.4"
	},
	"type": "module",
	"dependencies": {
		"@aws-sdk/client-secrets-manager": "^3.491.0",
		"@aws-sdk/client-ses": "^3.470.0",
		"@aws-sdk/client-ssm": "^3.490.0",
		"@axiomhq/js": "^1.0.0-rc.1",
		"@internationalized/date": "^3.5.0",
		"@lucia-auth/adapter-prisma": "^3.0.2",
		"@prisma/client": "^5.7.0",
		"@sveltejs/adapter-node": "^2.0.2",
		"bits-ui": "^0.11.8",
		"clsx": "^2.0.0",
		"cmdk-sv": "^0.0.12",
		"formsnap": "^0.4.1",
		"lucia": "^2.7.4",
		"lucide-svelte": "^0.297.0",
		"mode-watcher": "^0.1.2",
		"nodemailer": "^6.9.7",
		"svelte-sonner": "^0.3.11",
		"tailwind-variants": "^0.1.18",
		"vaul-svelte": "^0.0.6"
	}
}
